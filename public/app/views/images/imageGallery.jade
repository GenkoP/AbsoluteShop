.my-container
	h2 Image gallery
	.img-conteiner(ng-repeat="image in gallery")
		div.img-remove Remove
		div.img-remove.pull-right Update
		div(ng-show="identity.isAuthenticated()")
			a(href="#/admin/images/{{ image._id }}")
				img.thumbnail.img-gallery(src="{{ image.url }}", alt="Image" height="200" , weight="200")
		div(ng-hide="identity.isAuthenticated()")
			img.thumbnail.img-gallery(src="{{ image.url }}", alt="Image" height="200" , weight="200")